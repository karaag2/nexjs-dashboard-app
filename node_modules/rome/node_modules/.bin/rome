#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Administrateur/AppData/Local/pnpm/global/5/.pnpm/rome@12.1.3/node_modules/rome/bin/node_modules:/mnt/c/Users/Administrateur/AppData/Local/pnpm/global/5/.pnpm/rome@12.1.3/node_modules/rome/node_modules:/mnt/c/Users/Administrateur/AppData/Local/pnpm/global/5/.pnpm/rome@12.1.3/node_modules:/mnt/c/Users/Administrateur/AppData/Local/pnpm/global/5/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Administrateur/AppData/Local/pnpm/global/5/.pnpm/rome@12.1.3/node_modules/rome/bin/node_modules:/mnt/c/Users/Administrateur/AppData/Local/pnpm/global/5/.pnpm/rome@12.1.3/node_modules/rome/node_modules:/mnt/c/Users/Administrateur/AppData/Local/pnpm/global/5/.pnpm/rome@12.1.3/node_modules:/mnt/c/Users/Administrateur/AppData/Local/pnpm/global/5/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/rome" "$@"
else
  exec node  "$basedir/../../bin/rome" "$@"
fi
